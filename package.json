{
  "name": "getzane",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bunx --bun vite build",
    "preview": "bunx --bun vite preview --port 4173",
    "deploy": "bun run build && bunx wrangler pages deploy dist --project-name getzane --branch main",
    "migrate": "bunx wrangler d1 migrations apply zane-waitlist --remote",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,svelte}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "sv-router": "^0.13.0",
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@types/node": "^25.1.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^3.14.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^15.2.11",
    "svelte-eslint-parser": "^1.4.1",
    "typescript": "^5.7.0",
    "vite": "^6.0.0"
  }
}
